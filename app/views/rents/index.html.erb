<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-3">
      <h2>My rentals</h2>

      <% @rents.each do |rent| %>
        <p>User: <%= rent.user_id %></p>
        <p>Galaxy: <%= rent.galaxy_id %></p>
        <p>Start Date: <%= rent.start_date %></p>
        <p>End Date: <%= rent.end_date %></p>
        <%= link_to "Edit", edit_rent_path(rent), class: "btn btn-ghost" %>
      <% end %>
    </div>
  </div>
</div>
